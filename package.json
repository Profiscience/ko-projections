{
  "name": "ko-projections",
  "version": "1.0.0",
  "description": "Observable Lodash Chains for KnockoutJS",
  "main": "dist/ko-projections.js",
  "scripts": {
    "build": "webpack && uglifyjs dist/ko-projections.js -o dist/ko-projections.min.js",
    "watch": "webpack --watch",
    "prepublish": "npm run build && npm run example:build",
    "stage-dist": "git add dist",
    "test": "karma start karma.travis.js",
    "test:watch": "karma start"
  },
  "precommit": [
    "build",
    "stage-dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/profiscience/ko-projections.git"
  },
  "keywords": [
    "knockout",
    "lodash",
    "ko",
    "projection",
    "underscore"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/profiscience/ko-projections/issues"
  },
  "homepage": "https://github.com/profiscience/ko-projections#readme",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "codeclimate-test-reporter": "^0.3.0",
    "eslint": "^2.1.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "knockout": "^3.4.0",
    "lodash": "^4.11.1",
    "pre-commit": "^1.1.2",
    "tape": "^4.4.0",
    "uglify-js": "^2.4.24",
    "webpack": "^1.11.0"
  },
  "peerDependencies": {
    "knockout": "^3.3.0",
    "lodash": "^4.0.0"
  }
}
