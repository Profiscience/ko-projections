{
  "name": "ko-projections",
  "version": "1.1.0",
  "description": "Observable Lodash Chains for KnockoutJS",
  "main": "dist/ko-projections.js",
  "scripts": {
    "build": "webpack && uglifyjs dist/ko-projections.js -o dist/ko-projections.min.js",
    "watch": "webpack --watch",
    "prepublish": "npm run build",
    "stage-dist": "git add dist",
    "test": "export NODE_ENV=test; nyc --reporter=lcov --reporter=html ava --verbose --serial",
    "test:watch": "ava --watch"
  },
  "precommit": [
    "build",
    "stage-dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/profiscience/ko-projections.git"
  },
  "keywords": [
    "knockout",
    "lodash",
    "ko",
    "projection",
    "underscore"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/profiscience/ko-projections/issues"
  },
  "homepage": "https://github.com/profiscience/ko-projections#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "coveralls": "^2.11.9",
    "eslint": "^2.1.0",
    "eslint-config-profiscience": "^1.0.1",
    "knockout": "^3.4.0",
    "lodash": "^4.11.1",
    "nyc": "^6.6.1",
    "pre-commit": "^1.1.2",
    "tape": "^4.4.0",
    "uglify-js": "^2.4.24",
    "webpack": "^1.11.0"
  },
  "peerDependencies": {
    "knockout": "^3.3.0",
    "lodash": "^4.0.0"
  }
}
